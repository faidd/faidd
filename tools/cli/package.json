{
  "name": "faidd",
  "version": "0.1.8",
  "type": "module",
  "private": false,
  "description": "FAIDD Official CLI - The orchestration engine for AI-Driven Development",
  "author": "FAIDD Team",
  "license": "Apache-2.0",
  "homepage": "https://github.com/faidd/faidd#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/faidd/faidd.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc -b && cp -r src/modules dist/ && cp -r src/installers/templates dist/installers/ && cp src/installers/install-messages.md dist/installers/",
    "dev": "tsc -b --watch"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "bin": {
    "faidd": "./dist/index.js"
  },
  "dependencies": {
    "@faidd/logic": "workspace:*",
    "@faidd/types": "workspace:*",
    "boxen": "^8.0.1",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "csv-parse": "^6.1.0",
    "figlet": "^1.10.0",
    "fs-extra": "^11.2.0",
    "gradient-string": "^3.0.0",
    "inquirer": "^9.3.8",
    "listr2": "^10.1.0",
    "ora": "^9.3.0",
    "yaml": "^2.8.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/figlet": "^1.7.0",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.9",
    "@types/node": "^20.0.0",
    "execa": "^9.0.0",
    "vitest": "^2.1.0"
  }
}
